<?xml version="1.0"?>

<driver>

   <name>CFW8</name>
   <type>FilterWheel</type>

   <cmdline>builtin</cmdline>

   <filterset>
      <filter>
         <index>0</index>
         <name>clear</name>
      </filter>

      <filter>
         <index>1</index>
         <name>red</name>
      </filter>

      <filter>
         <index>2</index>
         <name>green</name>
      </filter>

      <filter>
         <index>3</index>
         <name>blue</name>
      </filter>

      <filter>
         <index>4</index>
         <name>lunar</name>
      </filter>

   </filterset>

</driver>
